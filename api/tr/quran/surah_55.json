{
  "number": 55,
  "name": "سُورَةُ الرَّحۡمَٰن",
  "englishName": "Rahmân",
  "englishNameTranslation": "Rahmân",
  "revelationType": "Medeni",
  "ayahs": [
    {
      "number": 4902,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ ٱلرَّحْمَٰنُ",
      "translation": "Rahman olan Allah Kuran'ı öğretti;",
      "numberInSurah": 1,
      "juz": 27,
      "page": 531
    },
    {
      "number": 4903,
      "text": "عَلَّمَ ٱلْقُرْءَانَ",
      "translation": "Rahman olan Allah Kuran'ı öğretti;",
      "numberInSurah": 2,
      "juz": 27,
      "page": 531
    },
    {
      "number": 4904,
      "text": "خَلَقَ ٱلْإِنسَٰنَ",
      "translation": "İnsanı yarattı, ona konuşmayı öğretti.",
      "numberInSurah": 3,
      "juz": 27,
      "page": 531
    },
    {
      "number": 4905,
      "text": "عَلَّمَهُ ٱلْبَيَانَ",
      "translation": "İnsanı yarattı, ona konuşmayı öğretti.",
      "numberInSurah": 4,
      "juz": 27,
      "page": 531
    },
    {
      "number": 4906,
      "text": "ٱلشَّمْسُ وَٱلْقَمَرُ بِحُسْبَانٍۢ",
      "translation": "Güneş ve ayın hareketleri bir hesaba göredir.",
      "numberInSurah": 5,
      "juz": 27,
      "page": 531
    },
    {
      "number": 4907,
      "text": "وَٱلنَّجْمُ وَٱلشَّجَرُ يَسْجُدَانِ",
      "translation": "Bitkiler ve ağaçlar O'nun buyruğuna boyun eğerler.",
      "numberInSurah": 6,
      "juz": 27,
      "page": 531
    },
    {
      "number": 4908,
      "text": "وَٱلسَّمَآءَ رَفَعَهَا وَوَضَعَ ٱلْمِيزَانَ",
      "translation": "O, göğü yükseltmiştir; tartıyı koymuştur.",
      "numberInSurah": 7,
      "juz": 27,
      "page": 531
    },
    {
      "number": 4909,
      "text": "أَلَّا تَطْغَوْا۟ فِى ٱلْمِيزَانِ",
      "translation": "Artık tartıda tecavüz etmeyin.",
      "numberInSurah": 8,
      "juz": 27,
      "page": 531
    },
    {
      "number": 4910,
      "text": "وَأَقِيمُوا۟ ٱلْوَزْنَ بِٱلْقِسْطِ وَلَا تُخْسِرُوا۟ ٱلْمِيزَانَ",
      "translation": "Tartmayı doğru yapın, tartıyı eksik tutmayın.",
      "numberInSurah": 9,
      "juz": 27,
      "page": 531
    },
    {
      "number": 4911,
      "text": "وَٱلْأَرْضَ وَضَعَهَا لِلْأَنَامِ",
      "translation": "Allah, yeri yaratıkları için meydana getirmiştir.",
      "numberInSurah": 10,
      "juz": 27,
      "page": 531
    },
    {
      "number": 4912,
      "text": "فِيهَا فَٰكِهَةٌۭ وَٱلنَّخْلُ ذَاتُ ٱلْأَكْمَامِ",
      "translation": "Orada meyveler, salkımlı hurma ağaçları, kabuklu taneler, güzel kokulu otlar vardır.",
      "numberInSurah": 11,
      "juz": 27,
      "page": 531
    },
    {
      "number": 4913,
      "text": "وَٱلْحَبُّ ذُو ٱلْعَصْفِ وَٱلرَّيْحَانُ",
      "translation": "Orada meyveler, salkımlı hurma ağaçları, kabuklu taneler, güzel kokulu otlar vardır.",
      "numberInSurah": 12,
      "juz": 27,
      "page": 531
    },
    {
      "number": 4914,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Ey insanlar ve cinler! Öyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 13,
      "juz": 27,
      "page": 531
    },
    {
      "number": 4915,
      "text": "خَلَقَ ٱلْإِنسَٰنَ مِن صَلْصَٰلٍۢ كَٱلْفَخَّارِ",
      "translation": "O, insanı pişmiş çamur gibi kuru balçıktan yaratmıştır.",
      "numberInSurah": 14,
      "juz": 27,
      "page": 531
    },
    {
      "number": 4916,
      "text": "وَخَلَقَ ٱلْجَآنَّ مِن مَّارِجٍۢ مِّن نَّارٍۢ",
      "translation": "Cinleri de yalın bir alevden yaratmıştır.",
      "numberInSurah": 15,
      "juz": 27,
      "page": 531
    },
    {
      "number": 4917,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken; Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 16,
      "juz": 27,
      "page": 531
    },
    {
      "number": 4918,
      "text": "رَبُّ ٱلْمَشْرِقَيْنِ وَرَبُّ ٱلْمَغْرِبَيْنِ",
      "translation": "O, iki doğunun Rabbidir, iki batının Rabbidir.",
      "numberInSurah": 17,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4919,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 18,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4920,
      "text": "مَرَجَ ٱلْبَحْرَيْنِ يَلْتَقِيَانِ",
      "translation": "Acı ve tatlı sulu iki denizi birbirine kavuşmamak üzere salıvermiştir.",
      "numberInSurah": 19,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4921,
      "text": "بَيْنَهُمَا بَرْزَخٌۭ لَّا يَبْغِيَانِ",
      "translation": "Aralarında bir engel vardır; birbirinin sınırını aşamazlar.",
      "numberInSurah": 20,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4922,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 21,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4923,
      "text": "يَخْرُجُ مِنْهُمَا ٱللُّؤْلُؤُ وَٱلْمَرْجَانُ",
      "translation": "Bu iki denizden de inci ve mercan çıkar.",
      "numberInSurah": 22,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4924,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 23,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4925,
      "text": "وَلَهُ ٱلْجَوَارِ ٱلْمُنشَـَٔاتُ فِى ٱلْبَحْرِ كَٱلْأَعْلَٰمِ",
      "translation": "Denizde yürüyen dağlar gibi gemiler O'nundur.",
      "numberInSurah": 24,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4926,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 25,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4927,
      "text": "كُلُّ مَنْ عَلَيْهَا فَانٍۢ",
      "translation": "Yeryüzünde bulunan her şey fanidir.",
      "numberInSurah": 26,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4928,
      "text": "وَيَبْقَىٰ وَجْهُ رَبِّكَ ذُو ٱلْجَلَٰلِ وَٱلْإِكْرَامِ",
      "translation": "Ancak, yüce ve cömert olan Rabbinin varlığı bakidir.",
      "numberInSurah": 27,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4929,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 28,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4930,
      "text": "يَسْـَٔلُهُۥ مَن فِى ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ ۚ كُلَّ يَوْمٍ هُوَ فِى شَأْنٍۢ",
      "translation": "Göklerde ve yerde olan kimseler her şeyi O'ndan isterler; O her an kainata tasarruf etmektedir.",
      "numberInSurah": 29,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4931,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 30,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4932,
      "text": "سَنَفْرُغُ لَكُمْ أَيُّهَ ٱلثَّقَلَانِ",
      "translation": "Ey insan ve cin toplulukları! Sizin de hesabınızı ele alacağız.",
      "numberInSurah": 31,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4933,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 32,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4934,
      "text": "يَٰمَعْشَرَ ٱلْجِنِّ وَٱلْإِنسِ إِنِ ٱسْتَطَعْتُمْ أَن تَنفُذُوا۟ مِنْ أَقْطَارِ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ فَٱنفُذُوا۟ ۚ لَا تَنفُذُونَ إِلَّا بِسُلْطَٰنٍۢ",
      "translation": "Ey cin ve insan toplulukları! Göklerin ve yerin çevresini aşıp geçmeye gücünüz yetiyorsa geçin! Ama Allah'ın verdiği bir güç olmaksızın geçemezsiniz ki!",
      "numberInSurah": 33,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4935,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 34,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4936,
      "text": "يُرْسَلُ عَلَيْكُمَا شُوَاظٌۭ مِّن نَّارٍۢ وَنُحَاسٌۭ فَلَا تَنتَصِرَانِ",
      "translation": "Ey insanlar ve cinler! Üzerinize dumansız bir alev ve ateşsiz bir duman gönderilir de kurtulamazsınız.",
      "numberInSurah": 35,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4937,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 36,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4938,
      "text": "فَإِذَا ٱنشَقَّتِ ٱلسَّمَآءُ فَكَانَتْ وَرْدَةًۭ كَٱلدِّهَانِ",
      "translation": "Gök yarılıp da, gül gibi kızardığı, yağ gibi eridiği zaman haliniz nice olur?",
      "numberInSurah": 37,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4939,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 38,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4940,
      "text": "فَيَوْمَئِذٍۢ لَّا يُسْـَٔلُ عَن ذَنۢبِهِۦٓ إِنسٌۭ وَلَا جَآنٌّۭ",
      "translation": "O gün ne insana ve ne cine suçu sorulur.",
      "numberInSurah": 39,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4941,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 40,
      "juz": 27,
      "page": 532
    },
    {
      "number": 4942,
      "text": "يُعْرَفُ ٱلْمُجْرِمُونَ بِسِيمَٰهُمْ فَيُؤْخَذُ بِٱلنَّوَٰصِى وَٱلْأَقْدَامِ",
      "translation": "Suçlular simalarından tanınırlar da, alın saçlarından ve ayaklarından yakalanırlar.",
      "numberInSurah": 41,
      "juz": 27,
      "page": 533
    },
    {
      "number": 4943,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 42,
      "juz": 27,
      "page": 533
    },
    {
      "number": 4944,
      "text": "هَٰذِهِۦ جَهَنَّمُ ٱلَّتِى يُكَذِّبُ بِهَا ٱلْمُجْرِمُونَ",
      "translation": "İşte suçluların yalanladıkları cehennem budur.",
      "numberInSurah": 43,
      "juz": 27,
      "page": 533
    },
    {
      "number": 4945,
      "text": "يَطُوفُونَ بَيْنَهَا وَبَيْنَ حَمِيمٍ ءَانٍۢ",
      "translation": "Onlar, cehennem ateşiyle kaynar su arasında dolaşır dururlar.",
      "numberInSurah": 44,
      "juz": 27,
      "page": 533
    },
    {
      "number": 4946,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 45,
      "juz": 27,
      "page": 533
    },
    {
      "number": 4947,
      "text": "وَلِمَنْ خَافَ مَقَامَ رَبِّهِۦ جَنَّتَانِ",
      "translation": "Rabbine karşı durmaktan korkan kimseye iki cennet vardır.",
      "numberInSurah": 46,
      "juz": 27,
      "page": 533
    },
    {
      "number": 4948,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 47,
      "juz": 27,
      "page": 533
    },
    {
      "number": 4949,
      "text": "ذَوَاتَآ أَفْنَانٍۢ",
      "translation": "Bu iki cennet türlü ağaçlarla doludur.",
      "numberInSurah": 48,
      "juz": 27,
      "page": 533
    },
    {
      "number": 4950,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 49,
      "juz": 27,
      "page": 533
    },
    {
      "number": 4951,
      "text": "فِيهِمَا عَيْنَانِ تَجْرِيَانِ",
      "translation": "Bu cennetlerden akan iki kaynak vardır.",
      "numberInSurah": 50,
      "juz": 27,
      "page": 533
    },
    {
      "number": 4952,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 51,
      "juz": 27,
      "page": 533
    },
    {
      "number": 4953,
      "text": "فِيهِمَا مِن كُلِّ فَٰكِهَةٍۢ زَوْجَانِ",
      "translation": "Bu cennetlerde türlü meyveden çift çift vardır.",
      "numberInSurah": 52,
      "juz": 27,
      "page": 533
    },
    {
      "number": 4954,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 53,
      "juz": 27,
      "page": 533
    },
    {
      "number": 4955,
      "text": "مُتَّكِـِٔينَ عَلَىٰ فُرُشٍۭ بَطَآئِنُهَا مِنْ إِسْتَبْرَقٍۢ ۚ وَجَنَى ٱلْجَنَّتَيْنِ دَانٍۢ",
      "translation": "Orada, örtüleri parlak atlastan yataklara yaslanırlar; iki cennetin meyvelerini de kolayca toplarlar.",
      "numberInSurah": 54,
      "juz": 27,
      "page": 533
    },
    {
      "number": 4956,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 55,
      "juz": 27,
      "page": 533
    },
    {
      "number": 4957,
      "text": "فِيهِنَّ قَٰصِرَٰتُ ٱلطَّرْفِ لَمْ يَطْمِثْهُنَّ إِنسٌۭ قَبْلَهُمْ وَلَا جَآنٌّۭ",
      "translation": "Orada, bakışlarını yalnız eşlerine çevirmiş, daha önce ne insan ve ne de cinlerin dokunmuş olduğu eşler vardır.",
      "numberInSurah": 56,
      "juz": 27,
      "page": 533
    },
    {
      "number": 4958,
      "text": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
      "translation": "Öyleyken, Rabbinizin nimetlerinden hangisini yalanlarsınız?",
      "numberInSurah": 57,
      "juz": 27,
      "page": 533
    },
