{
  "number": 96,
  "name": "سُورَةُ العَلَقِ",
  "englishName": "Alak",
  "englishNameTranslation": "Alak",
  "revelationType": "Mekki",
  "ayahs": [
    {
      "number": 6107,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ ٱقْرَأْ بِٱسْمِ رَبِّكَ ٱلَّذِى خَلَقَ",
      "translation": "Yaratan Rabbinin adıyla oku!",
      "numberInSurah": 1,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6108,
      "text": "خَلَقَ ٱلْإِنسَٰنَ مِنْ عَلَقٍ",
      "translation": "O, insanı pıhtılaşmış kandan (alak'tan) yarattı.",
      "numberInSurah": 2,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6109,
      "text": "ٱقْرَأْ وَرَبُّكَ ٱلْأَكْرَمُ",
      "translation": "Oku! Kalemle öğreten, insana bilmediğini bildiren Rabbin, en büyük kerem sahibidir.",
      "numberInSurah": 3,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6110,
      "text": "ٱلَّذِى عَلَّمَ بِٱلْقَلَمِ",
      "translation": "Oku! Kalemle öğreten, insana bilmediğini bildiren Rabbin, en büyük kerem sahibidir.",
      "numberInSurah": 4,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6111,
      "text": "عَلَّمَ ٱلْإِنسَٰنَ مَا لَمْ يَعْلَمْ",
      "translation": "Oku! Kalemle öğreten, insana bilmediğini bildiren Rabbin, en büyük kerem sahibidir.",
      "numberInSurah": 5,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6112,
      "text": "كَلَّآ إِنَّ ٱلْإِنسَٰنَ لَيَطْغَىٰٓ",
      "translation": "Ama, insanoğlu kendini müstağni sayarak azgınlık eder.",
      "numberInSurah": 6,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6113,
      "text": "أَن رَّءَاهُ ٱسْتَغْنَىٰٓ",
      "translation": "Ama, insanoğlu kendini müstağni sayarak azgınlık eder.",
      "numberInSurah": 7,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6114,
      "text": "إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجْعَىٰٓ",
      "translation": "Dönüş şüphesiz Rabbinedir.",
      "numberInSurah": 8,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6115,
      "text": "أَرَءَيْتَ ٱلَّذِى يَنْهَىٰ",
      "translation": "Sen, namaz kılan kulu bundan menedeni gördün mü?",
      "numberInSurah": 9,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6116,
      "text": "عَبْدًا إِذَا صَلَّىٰٓ",
      "translation": "Sen, namaz kılan kulu bundan menedeni gördün mü?",
      "numberInSurah": 10,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6117,
      "text": "أَرَءَيْتَ إِن كَانَ عَلَى ٱلْهُدَىٰٓ",
      "translation": "Söyle bakalım, o kul doğru yolda giden veya Allah'a karşı gelmekten sakınmayı buyuran bir kimse olsun; veya söyle, yalanlayıp yüz çeviren birisi olsun",
      "numberInSurah": 11,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6118,
      "text": "أَوْ أَمَرَ بِٱلتَّقْوَىٰٓ",
      "translation": "Söyle bakalım, o kul doğru yolda giden veya Allah'a karşı gelmekten sakınmayı buyuran bir kimse olsun; veya söyle, yalanlayıp yüz çeviren birisi olsun",
      "numberInSurah": 12,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6119,
      "text": "أَرَءَيْتَ إِن كَذَّبَ وَتَوَلَّىٰٓ",
      "translation": "Söyle bakalım, o kul doğru yolda giden veya Allah'a karşı gelmekten sakınmayı buyuran bir kimse olsun; veya söyle, yalanlayıp yüz çeviren birisi olsun",
      "numberInSurah": 13,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6120,
      "text": "أَلَمْ يَعْلَم بِأَنَّ ٱللَّهَ يَرَىٰ",
      "translation": "Allah'ın her şeyi görmekte olduğunu bilmez mi?",
      "numberInSurah": 14,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6121,
      "text": "كَلَّا لَئِن لَّمْ يَنتَهِ لَنَسْفَعًۢا بِٱلنَّاصِيَةِ",
      "translation": "Ama bundan vazgeçmezse, and olsun ki, onu perçeminden,",
      "numberInSurah": 15,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6122,
      "text": "نَاصِيَةٍۢ كَٰذِبَةٍ خَاطِئَةٍۢ",
      "translation": "Yalancı ve günahkar perçeminden cehenneme sürükleriz.",
      "numberInSurah": 16,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6123,
      "text": "فَلْيَدْعُ نَادِيَهُۥ",
      "translation": "O zaman, kafadarlarını çağırsın,",
      "numberInSurah": 17,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6124,
      "text": "سَنَدْعُ ٱلزَّبَانِيَةَ",
      "translation": "Biz de zebanileri çağıracağız.",
      "numberInSurah": 18,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6125,
      "text": "كَلَّا لَا تُطِعْهُ وَٱسْجُدْ وَٱقْتَرِب ۩",
      "translation": "Sakın ona uyma; sen secde et, Rabbine yaklaş.",
      "numberInSurah": 19,
      "juz": 30,
      "page": 597
    }
  ]
}