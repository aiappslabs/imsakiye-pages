{
  "number": 93,
  "name": "سُورَةُ الضُّحَىٰ",
  "englishName": "Duhâ",
  "englishNameTranslation": "Kuşluk Vakti",
  "revelationType": "Mekki",
  "ayahs": [
    {
      "number": 6080,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلضُّحَىٰ",
      "translation": "Kuşluk vaktine and olsun;",
      "numberInSurah": 1,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6081,
      "text": "وَٱلَّيْلِ إِذَا سَجَىٰ",
      "translation": "Sükun erdiği zaman geceye and olsun ki,",
      "numberInSurah": 2,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6082,
      "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ",
      "translation": "Rabbin seni ne bıraktı ve ne de sana darıldı.",
      "numberInSurah": 3,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6083,
      "text": "وَلَلْءَاخِرَةُ خَيْرٌۭ لَّكَ مِنَ ٱلْأُولَىٰ",
      "translation": "Doğrusu ahiret senin için dünyadan daha hayırlıdır.",
      "numberInSurah": 4,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6084,
      "text": "وَلَسَوْفَ يُعْطِيكَ رَبُّكَ فَتَرْضَىٰٓ",
      "translation": "Rabbin şüphesiz sana verecek ve sen de hoşnut olacaksın.",
      "numberInSurah": 5,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6085,
      "text": "أَلَمْ يَجِدْكَ يَتِيمًۭا فَـَٔاوَىٰ",
      "translation": "Seni öksüz bulup da barındırmadı mı?",
      "numberInSurah": 6,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6086,
      "text": "وَوَجَدَكَ ضَآلًّۭا فَهَدَىٰ",
      "translation": "Seni şaşırmış bulup, doğru yola eriştirmedi mi?",
      "numberInSurah": 7,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6087,
      "text": "وَوَجَدَكَ عَآئِلًۭا فَأَغْنَىٰ",
      "translation": "Seni fakir bulup zenginleştirmedi mi?",
      "numberInSurah": 8,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6088,
      "text": "فَأَمَّا ٱلْيَتِيمَ فَلَا تَقْهَرْ",
      "translation": "Öyleyse sakın öksüze kötü muamele etme;",
      "numberInSurah": 9,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6089,
      "text": "وَأَمَّا ٱلسَّآئِلَ فَلَا تَنْهَرْ",
      "translation": "Ve sakın bir şey isteyeni azarlama;",
      "numberInSurah": 10,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6090,
      "text": "وَأَمَّا بِنِعْمَةِ رَبِّكَ فَحَدِّثْ",
      "translation": "Yalnızca Rabbinin nimetini anlat.",
      "numberInSurah": 11,
      "juz": 30,
      "page": 596
    }
  ]
}