{
  "number": 86,
  "name": "سُورَةُ الطَّارِقِ",
  "englishName": "At-Taariq",
  "englishNameTranslation": "The Morning Star",
  "revelationType": "Meccan",
  "ayahs": [
    {
      "number": 5932,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلسَّمَآءِ وَٱلطَّارِقِ",
      "translation": "Göğe ve Tarık'a and olsun;",
      "numberInSurah": 1,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5933,
      "text": "وَمَآ أَدْرَىٰكَ مَا ٱلطَّارِقُ",
      "translation": "Tarık'ın ne olduğunu sen bilir misin?",
      "numberInSurah": 2,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5934,
      "text": "ٱلنَّجْمُ ٱلثَّاقِبُ",
      "translation": "O, (ışığıyla karanlığı) delen yıldızdır.",
      "numberInSurah": 3,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5935,
      "text": "إِن كُلُّ نَفْسٍۢ لَّمَّا عَلَيْهَا حَافِظٌۭ",
      "translation": "Üzerinde gözetici olmayan kimse yoktur.",
      "numberInSurah": 4,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5936,
      "text": "فَلْيَنظُرِ ٱلْإِنسَٰنُ مِمَّ خُلِقَ",
      "translation": "Öyleyse insan neden yaratıldığına bir baksın.",
      "numberInSurah": 5,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5937,
      "text": "خُلِقَ مِن مَّآءٍۢ دَافِقٍۢ",
      "translation": "O, erkek ve kadının beli ile kaburga kemikleri arasından atılagelen bir sudan yaratılmıştır.",
      "numberInSurah": 6,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5938,
      "text": "يَخْرُجُ مِنۢ بَيْنِ ٱلصُّلْبِ وَٱلتَّرَآئِبِ",
      "translation": "O, erkek ve kadının beli ile kaburga kemikleri arasından atılagelen bir sudan yaratılmıştır.",
      "numberInSurah": 7,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5939,
      "text": "إِنَّهُۥ عَلَىٰ رَجْعِهِۦ لَقَادِرٌۭ",
      "translation": "Şüphesiz Allah, gizliliklerin ortaya çıkacağı gün, insanı tekrar yaratmaya Kadir'dir.",
      "numberInSurah": 8,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5940,
      "text": "يَوْمَ تُبْلَى ٱلسَّرَآئِرُ",
      "translation": "Şüphesiz Allah, gizliliklerin ortaya çıkacağı gün, insanı tekrar yaratmaya Kadir'dir.",
      "numberInSurah": 9,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5941,
      "text": "فَمَا لَهُۥ مِن قُوَّةٍۢ وَلَا نَاصِرٍۢ",
      "translation": "O gün, insanın gücü de, yardımcısı da olmaz.",
      "numberInSurah": 10,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5942,
      "text": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجْعِ",
      "translation": "Dönüşlü göğe ve yarılan yeryüzüne and olsun ki,",
      "numberInSurah": 11,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5943,
      "text": "وَٱلْأَرْضِ ذَاتِ ٱلصَّدْعِ",
      "translation": "Dönüşlü göğe ve yarılan yeryüzüne and olsun ki,",
      "numberInSurah": 12,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5944,
      "text": "إِنَّهُۥ لَقَوْلٌۭ فَصْلٌۭ",
      "translation": "Doğrusu bu Kuran kesin bir sözdür.",
      "numberInSurah": 13,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5945,
      "text": "وَمَا هُوَ بِٱلْهَزْلِ",
      "translation": "O, eğlence için değildir.",
      "numberInSurah": 14,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5946,
      "text": "إِنَّهُمْ يَكِيدُونَ كَيْدًۭا",
      "translation": "Gerçekten onlar düzen kuruyorlar.",
      "numberInSurah": 15,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5947,
      "text": "وَأَكِيدُ كَيْدًۭا",
      "translation": "Ben de bir düzen kurmaktayım.",
      "numberInSurah": 16,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5948,
      "text": "فَمَهِّلِ ٱلْكَٰفِرِينَ أَمْهِلْهُمْ رُوَيْدًۢا",
      "translation": "Sen inkarcılara mehil ver; onlara mukabeleyi biraz geri bırak.",
      "numberInSurah": 17,
      "juz": 30,
      "page": 591
    }
  ]
}