{
  "number": 86,
  "name": "سُورَةُ الطَّارِقِ",
  "englishName": "At-Taariq",
  "englishNameTranslation": "The Morning Star",
  "revelationType": "Meccan",
  "ayahs": [
    {
      "number": 5932,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلسَّمَآءِ وَٱلطَّارِقِ",
      "translation": "By the sky and the night comer -",
      "numberInSurah": 1,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5933,
      "text": "وَمَآ أَدْرَىٰكَ مَا ٱلطَّارِقُ",
      "translation": "And what can make you know what is the night comer?",
      "numberInSurah": 2,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5934,
      "text": "ٱلنَّجْمُ ٱلثَّاقِبُ",
      "translation": "It is the piercing star -",
      "numberInSurah": 3,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5935,
      "text": "إِن كُلُّ نَفْسٍۢ لَّمَّا عَلَيْهَا حَافِظٌۭ",
      "translation": "There is no soul but that it has over it a protector.",
      "numberInSurah": 4,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5936,
      "text": "فَلْيَنظُرِ ٱلْإِنسَٰنُ مِمَّ خُلِقَ",
      "translation": "So let man observe from what he was created.",
      "numberInSurah": 5,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5937,
      "text": "خُلِقَ مِن مَّآءٍۢ دَافِقٍۢ",
      "translation": "He was created from a fluid, ejected,",
      "numberInSurah": 6,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5938,
      "text": "يَخْرُجُ مِنۢ بَيْنِ ٱلصُّلْبِ وَٱلتَّرَآئِبِ",
      "translation": "Emerging from between the backbone and the ribs.",
      "numberInSurah": 7,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5939,
      "text": "إِنَّهُۥ عَلَىٰ رَجْعِهِۦ لَقَادِرٌۭ",
      "translation": "Indeed, Allah, to return him [to life], is Able.",
      "numberInSurah": 8,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5940,
      "text": "يَوْمَ تُبْلَى ٱلسَّرَآئِرُ",
      "translation": "The Day when secrets will be put on trial,",
      "numberInSurah": 9,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5941,
      "text": "فَمَا لَهُۥ مِن قُوَّةٍۢ وَلَا نَاصِرٍۢ",
      "translation": "Then man will have no power or any helper.",
      "numberInSurah": 10,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5942,
      "text": "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجْعِ",
      "translation": "By the sky which returns [rain]",
      "numberInSurah": 11,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5943,
      "text": "وَٱلْأَرْضِ ذَاتِ ٱلصَّدْعِ",
      "translation": "And [by] the earth which cracks open,",
      "numberInSurah": 12,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5944,
      "text": "إِنَّهُۥ لَقَوْلٌۭ فَصْلٌۭ",
      "translation": "Indeed, the Qur'an is a decisive statement,",
      "numberInSurah": 13,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5945,
      "text": "وَمَا هُوَ بِٱلْهَزْلِ",
      "translation": "And it is not amusement.",
      "numberInSurah": 14,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5946,
      "text": "إِنَّهُمْ يَكِيدُونَ كَيْدًۭا",
      "translation": "Indeed, they are planning a plan,",
      "numberInSurah": 15,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5947,
      "text": "وَأَكِيدُ كَيْدًۭا",
      "translation": "But I am planning a plan.",
      "numberInSurah": 16,
      "juz": 30,
      "page": 591
    },
    {
      "number": 5948,
      "text": "فَمَهِّلِ ٱلْكَٰفِرِينَ أَمْهِلْهُمْ رُوَيْدًۢا",
      "translation": "So allow time for the disbelievers. Leave them awhile.",
      "numberInSurah": 17,
      "juz": 30,
      "page": 591
    }
  ]
}