{
  "number": 93,
  "name": "سُورَةُ الضُّحَىٰ",
  "englishName": "Ad-Dhuhaa",
  "englishNameTranslation": "The Morning Hours",
  "revelationType": "Meccan",
  "ayahs": [
    {
      "number": 6080,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلضُّحَىٰ",
      "translation": "By the morning brightness",
      "numberInSurah": 1,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6081,
      "text": "وَٱلَّيْلِ إِذَا سَجَىٰ",
      "translation": "And [by] the night when it covers with darkness,",
      "numberInSurah": 2,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6082,
      "text": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ",
      "translation": "Your Lord has not taken leave of you, [O Muhammad], nor has He detested [you].",
      "numberInSurah": 3,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6083,
      "text": "وَلَلْءَاخِرَةُ خَيْرٌۭ لَّكَ مِنَ ٱلْأُولَىٰ",
      "translation": "And the Hereafter is better for you than the first [life].",
      "numberInSurah": 4,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6084,
      "text": "وَلَسَوْفَ يُعْطِيكَ رَبُّكَ فَتَرْضَىٰٓ",
      "translation": "And your Lord is going to give you, and you will be satisfied.",
      "numberInSurah": 5,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6085,
      "text": "أَلَمْ يَجِدْكَ يَتِيمًۭا فَـَٔاوَىٰ",
      "translation": "Did He not find you an orphan and give [you] refuge?",
      "numberInSurah": 6,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6086,
      "text": "وَوَجَدَكَ ضَآلًّۭا فَهَدَىٰ",
      "translation": "And He found you lost and guided [you],",
      "numberInSurah": 7,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6087,
      "text": "وَوَجَدَكَ عَآئِلًۭا فَأَغْنَىٰ",
      "translation": "And He found you poor and made [you] self-sufficient.",
      "numberInSurah": 8,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6088,
      "text": "فَأَمَّا ٱلْيَتِيمَ فَلَا تَقْهَرْ",
      "translation": "So as for the orphan, do not oppress [him].",
      "numberInSurah": 9,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6089,
      "text": "وَأَمَّا ٱلسَّآئِلَ فَلَا تَنْهَرْ",
      "translation": "And as for the petitioner, do not repel [him].",
      "numberInSurah": 10,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6090,
      "text": "وَأَمَّا بِنِعْمَةِ رَبِّكَ فَحَدِّثْ",
      "translation": "But as for the favor of your Lord, report [it].",
      "numberInSurah": 11,
      "juz": 30,
      "page": 596
    }
  ]
}