{
  "number": 81,
  "name": "سُورَةُ التَّكۡوِيرِ",
  "englishName": "At-Takwir",
  "englishNameTranslation": "The Overthrowing",
  "revelationType": "Meccan",
  "ayahs": [
    {
      "number": 5801,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ إِذَا ٱلشَّمْسُ كُوِّرَتْ",
      "translation": "Güneş dürülüp ışığı kalmadığı zaman;",
      "numberInSurah": 1,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5802,
      "text": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتْ",
      "translation": "Yıldızlar düşüp, söndüğü zaman;",
      "numberInSurah": 2,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5803,
      "text": "وَإِذَا ٱلْجِبَالُ سُيِّرَتْ",
      "translation": "Doğurması yaklaşmış develer başıboş bırakıldığı zaman;",
      "numberInSurah": 3,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5804,
      "text": "وَإِذَا ٱلْعِشَارُ عُطِّلَتْ",
      "translation": "Doğurması yaklaşmış develer başıboş bırakıldığı zaman;",
      "numberInSurah": 4,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5805,
      "text": "وَإِذَا ٱلْوُحُوشُ حُشِرَتْ",
      "translation": "Yabani hayvanlar bir araya toplatıldığı zaman;",
      "numberInSurah": 5,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5806,
      "text": "وَإِذَا ٱلْبِحَارُ سُجِّرَتْ",
      "translation": "Denizler kaynaştırıldığı zaman;",
      "numberInSurah": 6,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5807,
      "text": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتْ",
      "translation": "Canlar bedenlerle birleştirildiği zaman;",
      "numberInSurah": 7,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5808,
      "text": "وَإِذَا ٱلْمَوْءُۥدَةُ سُئِلَتْ",
      "translation": "Kız çocuğun hangi suçtan ötürü öldürüldüğü kendisine sorulduğu zaman;",
      "numberInSurah": 8,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5809,
      "text": "بِأَىِّ ذَنۢبٍۢ قُتِلَتْ",
      "translation": "Kız çocuğun hangi suçtan ötürü öldürüldüğü kendisine sorulduğu zaman;",
      "numberInSurah": 9,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5810,
      "text": "وَإِذَا ٱلصُّحُفُ نُشِرَتْ",
      "translation": "Amel defterleri açıldığı zaman;",
      "numberInSurah": 10,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5811,
      "text": "وَإِذَا ٱلسَّمَآءُ كُشِطَتْ",
      "translation": "Gök yerinden oynatıldığı zaman;",
      "numberInSurah": 11,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5812,
      "text": "وَإِذَا ٱلْجَحِيمُ سُعِّرَتْ",
      "translation": "Cehennem alevlendirildiği zaman;",
      "numberInSurah": 12,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5813,
      "text": "وَإِذَا ٱلْجَنَّةُ أُزْلِفَتْ",
      "translation": "Cennet yaklaştırıldığı zaman;",
      "numberInSurah": 13,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5814,
      "text": "عَلِمَتْ نَفْسٌۭ مَّآ أَحْضَرَتْ",
      "translation": "İnsanoğlu önceden ne hazırladığını görecektir.",
      "numberInSurah": 14,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5815,
      "text": "فَلَآ أُقْسِمُ بِٱلْخُنَّسِ",
      "translation": "Gündüz sinip geceleri gözüken gezegenlere and olsun;",
      "numberInSurah": 15,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5816,
      "text": "ٱلْجَوَارِ ٱلْكُنَّسِ",
      "translation": "Gündüz sinip geceleri gözüken gezegenlere and olsun;",
      "numberInSurah": 16,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5817,
      "text": "وَٱلَّيْلِ إِذَا عَسْعَسَ",
      "translation": "Kararmaya başlayan geceye and olsun;",
      "numberInSurah": 17,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5818,
      "text": "وَٱلصُّبْحِ إِذَا تَنَفَّسَ",
      "translation": "Ağarmaya başlayan sabaha and olsun ki,",
      "numberInSurah": 18,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5819,
      "text": "إِنَّهُۥ لَقَوْلُ رَسُولٍۢ كَرِيمٍۢ",
      "translation": "Bu Kuran, arşın sahibi katında değerli, güçlü, sözü dinlenen ve güvenilen şerefli bir elçinin getirdiği sözdür.",
      "numberInSurah": 19,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5820,
      "text": "ذِى قُوَّةٍ عِندَ ذِى ٱلْعَرْشِ مَكِينٍۢ",
      "translation": "Bu Kuran, arşın sahibi katında değerli, güçlü, sözü dinlenen ve güvenilen şerefli bir elçinin getirdiği sözdür.",
      "numberInSurah": 20,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5821,
      "text": "مُّطَاعٍۢ ثَمَّ أَمِينٍۢ",
      "translation": "Bu Kuran, arşın sahibi katında değerli, güçlü, sözü dinlenen ve güvenilen şerefli bir elçinin getirdiği sözdür.",
      "numberInSurah": 21,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5822,
      "text": "وَمَا صَاحِبُكُم بِمَجْنُونٍۢ",
      "translation": "Arkadaşınız (Muhammed) asla deli değildir.",
      "numberInSurah": 22,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5823,
      "text": "وَلَقَدْ رَءَاهُ بِٱلْأُفُقِ ٱلْمُبِينِ",
      "translation": "And olsun ki, o, Cebrail'i apaçık ufukta görmüştür.",
      "numberInSurah": 23,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5824,
      "text": "وَمَا هُوَ عَلَى ٱلْغَيْبِ بِضَنِينٍۢ",
      "translation": "Peygamber, görülmeyenler hakkında söylediklerinden ötürü töhmet altında tutulamaz.",
      "numberInSurah": 24,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5825,
      "text": "وَمَا هُوَ بِقَوْلِ شَيْطَٰنٍۢ رَّجِيمٍۢ",
      "translation": "Bu Kuran, kovulmuş şeytanın sözü olamaz.",
      "numberInSurah": 25,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5826,
      "text": "فَأَيْنَ تَذْهَبُونَ",
      "translation": "Nereye gidiyorsunuz?",
      "numberInSurah": 26,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5827,
      "text": "إِنْ هُوَ إِلَّا ذِكْرٌۭ لِّلْعَٰلَمِينَ",
      "translation": "Kuran, ancak aranızda doğru yola girmeyi dileyene ve alemlere bir öğüttür.",
      "numberInSurah": 27,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5828,
      "text": "لِمَن شَآءَ مِنكُمْ أَن يَسْتَقِيمَ",
      "translation": "Kuran, ancak aranızda doğru yola girmeyi dileyene ve alemlere bir öğüttür.",
      "numberInSurah": 28,
      "juz": 30,
      "page": 586
    },
    {
      "number": 5829,
      "text": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلْعَٰلَمِينَ",
      "translation": "Alemlerin Rabbi olan Allah dilemedikçe sizler bir şey dileyemezsiniz.",
      "numberInSurah": 29,
      "juz": 30,
      "page": 586
    }
  ]
}