{
  "number": 96,
  "name": "سُورَةُ العَلَقِ",
  "englishName": "Al-Alaq",
  "englishNameTranslation": "The Clot",
  "revelationType": "Meccan",
  "ayahs": [
    {
      "number": 6107,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ ٱقْرَأْ بِٱسْمِ رَبِّكَ ٱلَّذِى خَلَقَ",
      "translation": "Recite in the name of your Lord who created -",
      "numberInSurah": 1,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6108,
      "text": "خَلَقَ ٱلْإِنسَٰنَ مِنْ عَلَقٍ",
      "translation": "Created man from a clinging substance.",
      "numberInSurah": 2,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6109,
      "text": "ٱقْرَأْ وَرَبُّكَ ٱلْأَكْرَمُ",
      "translation": "Recite, and your Lord is the most Generous -",
      "numberInSurah": 3,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6110,
      "text": "ٱلَّذِى عَلَّمَ بِٱلْقَلَمِ",
      "translation": "Who taught by the pen -",
      "numberInSurah": 4,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6111,
      "text": "عَلَّمَ ٱلْإِنسَٰنَ مَا لَمْ يَعْلَمْ",
      "translation": "Taught man that which he knew not.",
      "numberInSurah": 5,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6112,
      "text": "كَلَّآ إِنَّ ٱلْإِنسَٰنَ لَيَطْغَىٰٓ",
      "translation": "No! [But] indeed, man transgresses",
      "numberInSurah": 6,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6113,
      "text": "أَن رَّءَاهُ ٱسْتَغْنَىٰٓ",
      "translation": "Because he sees himself self-sufficient.",
      "numberInSurah": 7,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6114,
      "text": "إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجْعَىٰٓ",
      "translation": "Indeed, to your Lord is the return.",
      "numberInSurah": 8,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6115,
      "text": "أَرَءَيْتَ ٱلَّذِى يَنْهَىٰ",
      "translation": "Have you seen the one who forbids",
      "numberInSurah": 9,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6116,
      "text": "عَبْدًا إِذَا صَلَّىٰٓ",
      "translation": "A servant when he prays?",
      "numberInSurah": 10,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6117,
      "text": "أَرَءَيْتَ إِن كَانَ عَلَى ٱلْهُدَىٰٓ",
      "translation": "Have you seen if he is upon guidance",
      "numberInSurah": 11,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6118,
      "text": "أَوْ أَمَرَ بِٱلتَّقْوَىٰٓ",
      "translation": "Or enjoins righteousness?",
      "numberInSurah": 12,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6119,
      "text": "أَرَءَيْتَ إِن كَذَّبَ وَتَوَلَّىٰٓ",
      "translation": "Have you seen if he denies and turns away -",
      "numberInSurah": 13,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6120,
      "text": "أَلَمْ يَعْلَم بِأَنَّ ٱللَّهَ يَرَىٰ",
      "translation": "Does he not know that Allah sees?",
      "numberInSurah": 14,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6121,
      "text": "كَلَّا لَئِن لَّمْ يَنتَهِ لَنَسْفَعًۢا بِٱلنَّاصِيَةِ",
      "translation": "No! If he does not desist, We will surely drag him by the forelock -",
      "numberInSurah": 15,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6122,
      "text": "نَاصِيَةٍۢ كَٰذِبَةٍ خَاطِئَةٍۢ",
      "translation": "A lying, sinning forelock.",
      "numberInSurah": 16,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6123,
      "text": "فَلْيَدْعُ نَادِيَهُۥ",
      "translation": "Then let him call his associates;",
      "numberInSurah": 17,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6124,
      "text": "سَنَدْعُ ٱلزَّبَانِيَةَ",
      "translation": "We will call the angels of Hell.",
      "numberInSurah": 18,
      "juz": 30,
      "page": 597
    },
    {
      "number": 6125,
      "text": "كَلَّا لَا تُطِعْهُ وَٱسْجُدْ وَٱقْتَرِب ۩",
      "translation": "No! Do not obey him. But prostrate and draw near [to Allah].",
      "numberInSurah": 19,
      "juz": 30,
      "page": 597
    }
  ]
}