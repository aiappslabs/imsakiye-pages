{
  "number": 94,
  "name": "سُورَةُ الشَّرۡحِ",
  "englishName": "Ash-Sharh",
  "englishNameTranslation": "The Consolation",
  "revelationType": "Meccan",
  "ayahs": [
    {
      "number": 6091,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ أَلَمْ نَشْرَحْ لَكَ صَدْرَكَ",
      "translation": "Did We not expand for you, [O Muhammad], your breast?",
      "numberInSurah": 1,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6092,
      "text": "وَوَضَعْنَا عَنكَ وِزْرَكَ",
      "translation": "And We removed from you your burden",
      "numberInSurah": 2,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6093,
      "text": "ٱلَّذِىٓ أَنقَضَ ظَهْرَكَ",
      "translation": "Which had weighed upon your back",
      "numberInSurah": 3,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6094,
      "text": "وَرَفَعْنَا لَكَ ذِكْرَكَ",
      "translation": "And raised high for you your repute.",
      "numberInSurah": 4,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6095,
      "text": "فَإِنَّ مَعَ ٱلْعُسْرِ يُسْرًا",
      "translation": "For indeed, with hardship [will be] ease.",
      "numberInSurah": 5,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6096,
      "text": "إِنَّ مَعَ ٱلْعُسْرِ يُسْرًۭا",
      "translation": "Indeed, with hardship [will be] ease.",
      "numberInSurah": 6,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6097,
      "text": "فَإِذَا فَرَغْتَ فَٱنصَبْ",
      "translation": "So when you have finished [your duties], then stand up [for worship].",
      "numberInSurah": 7,
      "juz": 30,
      "page": 596
    },
    {
      "number": 6098,
      "text": "وَإِلَىٰ رَبِّكَ فَٱرْغَب",
      "translation": "And to your Lord direct [your] longing.",
      "numberInSurah": 8,
      "juz": 30,
      "page": 596
    }
  ]
