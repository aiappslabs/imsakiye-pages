{
  "number": 107,
  "name": "سُورَةُ المَاعُونِ",
  "englishName": "Al-Maa'un",
  "englishNameTranslation": "Almsgiving",
  "revelationType": "Meccan",
  "ayahs": [
    {
      "number": 6198,
      "text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ أَرَءَيْتَ ٱلَّذِى يُكَذِّبُ بِٱلدِّينِ",
      "translation": "Have you seen the one who denies the Recompense?",
      "numberInSurah": 1,
      "juz": 30,
      "page": 602
    },
    {
      "number": 6199,
      "text": "فَذَٰلِكَ ٱلَّذِى يَدُعُّ ٱلْيَتِيمَ",
      "translation": "For that is the one who drives away the orphan",
      "numberInSurah": 2,
      "juz": 30,
      "page": 602
    },
    {
      "number": 6200,
      "text": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ",
      "translation": "And does not encourage the feeding of the poor.",
      "numberInSurah": 3,
      "juz": 30,
      "page": 602
    },
    {
      "number": 6201,
      "text": "فَوَيْلٌۭ لِّلْمُصَلِّينَ",
      "translation": "So woe to those who pray",
      "numberInSurah": 4,
      "juz": 30,
      "page": 602
    },
    {
      "number": 6202,
      "text": "ٱلَّذِينَ هُمْ عَن صَلَاتِهِمْ سَاهُونَ",
      "translation": "[But] who are heedless of their prayer -",
      "numberInSurah": 5,
      "juz": 30,
      "page": 602
    },
    {
      "number": 6203,
      "text": "ٱلَّذِينَ هُمْ يُرَآءُونَ",
      "translation": "Those who make show [of their deeds]",
      "numberInSurah": 6,
      "juz": 30,
      "page": 602
    },
    {
      "number": 6204,
      "text": "وَيَمْنَعُونَ ٱلْمَاعُونَ",
      "translation": "And withhold [simple] assistance.",
      "numberInSurah": 7,
      "juz": 30,
      "page": 602
    }
  ]
}